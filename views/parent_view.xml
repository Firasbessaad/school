<?xml version="1.0" ?>
<odoo>
        <record id="parent_inherit_res_parnter" model="ir.ui.view">
            <field name="name">parent.view.form.inherit</field>
            <field name="model">res.partner</field>
            <field name="inherit_id" ref="base.view_partner_form"/>
            <field name="arch" type="xml">
                <xpath expr="//span[@name='address_name']" position="attributes">
                    <attribute name="invisible">1</attribute>
                </xpath>
                <xpath expr="//field[@name='vat']" position="attributes">
                    <attribute name="invisible">1</attribute>
                </xpath>
                <xpath expr="//field[@name='type']" position="attributes">
                    <attribute name="invisible">1</attribute>
                </xpath>
            </field>
        </record>
        <!-- Action View Of school parent-->

        <record id="" model="ir.actions.act_window">
            <field name="name">Parent Profile</field>
            <field name="res_model">school.parent</field>
            <field name="view_mode">tree,form</field>
        </record>
        <record id="action_school_parent_form" model="ir.actions.act_window">
            <field name="name">Parents</field>
            <field name="res_model">res.partner</field>
            <field name="view_mode">tree,form</field>
            <!-- <field name="view_id" ref="parent_inherit_res_parnter"/>
            <field name="search_view_id" ref="view_thakafah_session_candidate_filter"/> -->
            <field name="domain">[('is_parent','=',True)]</field>
            <field name="context">{}</field>
            <field name="help" type="html">
                
            </field>
        </record>
</odoo>
